<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    
    <welcome-file-list>
        <welcome-file>TestForm.jsp</welcome-file>
    </welcome-file-list>

   <servlet>
       <servlet-name>TestServlet</servlet-name>
       <servlet-class>com.gcu.edu.TestServlet</servlet-class>
       <load-on-startup>1</load-on-startup>
   </servlet>

    <servlet-mapping>
        <servlet-name>TestServlet</servlet-name>
        <url-pattern>/testServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>NameServlet</servlet-name>
        <servlet-class>com.gcu.edu.NameServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>NameServlet</servlet-name>
        <url-pattern>/nameServlet</url-pattern>
    </servlet-mapping>


</web-app>